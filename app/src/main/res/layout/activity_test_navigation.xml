<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".test.activity.TestNavigationActivity">

    <TextView
        android:id="@+id/testView50"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:layout_gravity="center"
        android:text="GPS 위, 경도"
        android:textAlignment="center"
        android:textSize="25sp" />
    <TextView
        android:id="@+id/curLocationCoordTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="현재 위, 경도"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_gravity="center"
        android:text="다음 위, 경도"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/nextLocationCoordTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="다음 위, 경도"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="50dp"
        android:text="이전 위, 경도"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/prevLocationCoordTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="이전 위, 경도"
        android:textAlignment="center"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/nextDirectionTV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="95dp"
        android:text="다음 방향"
        android:textAlignment="center"
        android:textSize="25sp" />
</LinearLayout>